# AI Lead Generation Ultimate - Complete Environment Configuration
# This file contains ALL environment variables for the ultimate workflow with 10+ advanced features

#====================================================================================================
# CORE LICENSING & AUTHENTICATION
#====================================================================================================

# Commercial License Configuration (Required)
WORKFLOW_LICENSE_KEY=your_ultimate_license_key_here
LICENSE_API_KEY=your_license_api_key_here
N8N_INSTANCE_ID=leadgen_ultimate_001

#====================================================================================================
# PRIMARY API CREDENTIALS
#====================================================================================================

# Lead Discovery APIs
APOLLO_API_KEY=your_apollo_api_key_here

# Email Discovery & Verification
HUNTER_API_KEY=your_hunter_api_key_here

# Profile & Company Enrichment (Optional)
CLEARBIT_API_KEY=your_clearbit_api_key_here

# AI Processing
OPENAI_API_KEY=sk-your_openai_api_key_here
OPENAI_MODEL=gpt-4

#====================================================================================================
# MULTI-CHANNEL OUTREACH APIS
#====================================================================================================

# SMS & WhatsApp Outreach
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=+1234567890

# LinkedIn Automation Integration
LINKEDIN_AUTOMATION_WEBHOOK_URL=https://phantombuster.com/api/v1/webhook/your-webhook-id
# Alternative: TEXAU_WEBHOOK_URL, LEMLIST_WEBHOOK_URL, etc.

# Email SMTP Configuration for Follow-ups
SMTP_FROM_EMAIL=your-email@yourcompany.com
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your-email@yourcompany.com
SMTP_PASSWORD=your_app_specific_password

#====================================================================================================
# CRM INTEGRATIONS
#====================================================================================================

# CRM Provider Selection
CRM_PROVIDER=google_sheets    # Options: google_sheets, hubspot, salesforce, pipedrive

# Google Sheets (Default)
GOOGLE_SHEETS_ID=your_google_spreadsheet_id_here

# HubSpot Integration
HUBSPOT_API_KEY=your_hubspot_api_key
HUBSPOT_PORTAL_ID=your_portal_id

# Salesforce Integration
SALESFORCE_USERNAME=your_salesforce_username
SALESFORCE_PASSWORD=your_salesforce_password
SALESFORCE_SECURITY_TOKEN=your_security_token
SALESFORCE_INSTANCE_URL=https://your-instance.salesforce.com

# Pipedrive Integration
PIPEDRIVE_API_KEY=your_pipedrive_api_key
PIPEDRIVE_DOMAIN=your_company_domain

#====================================================================================================
# AIRTABLE DASHBOARD INTEGRATION
#====================================================================================================

# Airtable Configuration (Recommended for Ultimate Dashboard)
AIRTABLE_API_KEY=your_airtable_api_key
AIRTABLE_BASE_ID=your_airtable_base_id
AIRTABLE_BASE_URL=https://airtable.com/your_base_id

# Required Airtable Tables (create these exact names):
# - Leads Dashboard
# - Nurturing Sequences  
# - Lead Conversations
# - Deliverability Monitoring
# - Workflow Analytics

#====================================================================================================
# ZAPIER & WEBHOOK INTEGRATIONS
#====================================================================================================

# Zapier Gateway
ZAPIER_WEBHOOK_URL=https://hooks.zapier.com/hooks/catch/your-webhook-url

# Custom Webhook Endpoints
CUSTOM_WEBHOOK_ENDPOINT=https://your-custom-service.com/webhook
WEBHOOK_AUTHENTICATION_TOKEN=your_webhook_auth_token

#====================================================================================================
# SLACK NOTIFICATIONS (Multiple Channels)
#====================================================================================================

# Main Notifications Channel
SLACK_CHANNEL_ID=your_main_slack_channel_id

# Sales Team Notifications
SLACK_SALES_CHANNEL_ID=your_sales_slack_channel_id

# Technical Alerts Channel  
SLACK_TECH_CHANNEL_ID=your_tech_slack_channel_id

#====================================================================================================
# WORKFLOW CONFIGURATION
#====================================================================================================

# Execution Schedule
WORKFLOW_SCHEDULE=0 9 * * 1-5    # 9 AM weekdays (cron format)

# Performance & Scaling
BATCH_SIZE=15                    # Leads per batch (10-25 recommended)
MAX_LEADS_PER_RUN=150           # Maximum leads per execution

# Execution Modes
DEBUG_MODE=false                # Enable detailed logging
TEST_MODE=false                 # Run with sample data only

#====================================================================================================
# IDEAL CUSTOMER PROFILE (ICP) CONFIGURATION
#====================================================================================================

# Target Companies
ICP_TARGET_COMPANIES=OpenAI,Anthropic,Microsoft,Google,Amazon,Meta,Tesla,Stripe,Shopify,Notion

# ICP Description
ICP_DESCRIPTION=VP of Engineering, CTO, Head of AI, or Senior Engineering Manager at high-growth tech companies with strong engineering culture and AI/ML initiatives

# Job Title Filters
ICP_JOB_TITLES=CTO,Chief Technology Officer,VP Engineering,VP of Engineering,Head of AI,Head of Machine Learning,ML Engineer,AI Engineer,Engineering Manager,Senior Engineering Manager

# Industry Filters
ICP_INDUSTRIES=Technology,Software,Artificial Intelligence,Computer Software,Internet,SaaS,Machine Learning,Data Science

# Geographic Filters
ICP_LOCATIONS=United States,Canada,United Kingdom,Germany,France,Netherlands,Sweden,Australia,Singapore

# Company Size Range
ICP_COMPANY_SIZE_MIN=100
ICP_COMPANY_SIZE_MAX=1000

# Advanced ICP Filters
ICP_FUNDING_STAGE=Series A,Series B,Series C,Series D,Growth Stage
ICP_REVENUE_RANGE=10M-100M
ICP_TECH_STACK=Python,JavaScript,React,TypeScript,Node.js,AWS,Docker,Kubernetes,TensorFlow,PyTorch

#====================================================================================================
# AI & PERSONALIZATION CONFIGURATION
#====================================================================================================

# AI Model Settings
AI_PROMPT_TEMPLATE=saas-default-v2    # Options: saas-default-v2, ecom-focused-v1, enterprise-v1
OUTREACH_TONE=conversational         # Options: conversational, formal, persuasive
OUTREACH_LANGUAGE=EN                 # Default language (will auto-detect from location)

# AI Processing Parameters
OPENAI_TEMPERATURE=0.3              # AI creativity (0.0-1.0)
OPENAI_MAX_TOKENS=1000             # Max tokens per AI response
MIN_SCORE_THRESHOLD=7               # Minimum score for high-quality leads

#====================================================================================================
# FEATURE TOGGLES (Enable/Disable Advanced Features)
#====================================================================================================

# Multi-Channel Outreach
ENABLE_EMAIL_OUTREACH=true
ENABLE_SMS_OUTREACH=true           # Requires Twilio setup
ENABLE_LINKEDIN_AUTOMATION=true    # Requires webhook setup

# Lead Nurturing & Follow-ups  
ENABLE_LEAD_NURTURING=true
NURTURING_SEQUENCE_DAYS=2          # Days between follow-ups
NURTURING_MESSAGES_COUNT=3         # Number of follow-up messages

# Integration Features
ENABLE_ZAPIER_SYNC=true
ENABLE_AIRTABLE_SYNC=true
ENABLE_DELIVERABILITY_MONITOR=true

# Advanced Features
ENABLE_REP_ASSIGNMENT=true         # Sales rep routing
ENABLE_REPLY_ENGINE=true          # AI reply analysis

#====================================================================================================
# DELIVERABILITY & EMAIL MONITORING
#====================================================================================================

# Email Quota Management
DAILY_EMAIL_QUOTA=500             # Daily sending limit
EMAIL_WARMUP_ENABLED=true
WARMUP_INBOX_API_KEY=your_warmup_api_key

# Deliverability Monitoring
BOUNCE_RATE_THRESHOLD=3           # Alert if bounce rate > 3%
SPAM_RATE_THRESHOLD=1            # Alert if spam rate > 1%
MINIMUM_OPEN_RATE=20             # Alert if open rate < 20%

#====================================================================================================
# MULTI-LANGUAGE SUPPORT
#====================================================================================================

# Language Settings
ENABLE_MULTILINGUAL=true
FALLBACK_LANGUAGE=EN

# Language Mapping (automatically assigned by country)
# US,CA,UK,AU,SG = EN
# DE = DE (German)
# FR = FR (French)  
# ES = ES (Spanish)
# IT = IT (Italian)
# NL = NL (Dutch)
# SV = SV (Swedish)

#====================================================================================================
# SALES TEAM CONFIGURATION
#====================================================================================================

# Sales Rep Assignment (configure in workflow or via webhook)
# Format: rep_id:name:email:territories:industries:max_leads_per_day
SALES_REP_CONFIG_ENABLED=true

# Example configuration (you can modify in the workflow function):
# Sarah Johnson: US,CA | Technology,SaaS | 20 leads/day
# Michael Chen: UK,DE,FR | AI,ML | 15 leads/day  
# Emma Rodriguez: AU,SG | Enterprise,Cloud | 18 leads/day

#====================================================================================================
# RATE LIMITING & API OPTIMIZATION
#====================================================================================================

# API Rate Limits (requests per minute)
APOLLO_RATE_LIMIT=60
HUNTER_RATE_LIMIT=50
CLEARBIT_RATE_LIMIT=30
OPENAI_RATE_LIMIT=30
TWILIO_RATE_LIMIT=100

# Timeout Configuration (milliseconds)
API_TIMEOUT=30000               # 30 seconds
LICENSE_TIMEOUT=10000           # 10 seconds

# Retry Configuration
MAX_RETRIES=3
RETRY_DELAY_BASE=2000          # Base delay in milliseconds

#====================================================================================================
# ADVANCED ANALYTICS & TRACKING
#====================================================================================================

# Performance Tracking
PERFORMANCE_TRACKING=true
CONVERSION_TRACKING=true
ROI_TRACKING=true

# Custom Analytics
GOOGLE_ANALYTICS_ID=GA4-your-measurement-id
MIXPANEL_PROJECT_TOKEN=your_mixpanel_token

# A/B Testing
AB_TESTING_ENABLED=true
AB_TEST_SPLIT_RATIO=50          # 50/50 split for testing

#====================================================================================================
# SECURITY & COMPLIANCE
#====================================================================================================

# Data Retention
DATA_RETENTION_DAYS=90          # Days to keep workflow data
AUTO_CLEANUP_ENABLED=true       # Automatic data cleanup

# Privacy Compliance
GDPR_COMPLIANCE=true            # GDPR compliance features
CCPA_COMPLIANCE=true            # CCPA compliance features
DATA_PROCESSING_CONSENT=required # require, optional, automatic

# Security Settings
WEBHOOK_SECURITY_TOKEN=your_webhook_security_token
API_KEY_ROTATION_DAYS=90        # Rotate API keys every 90 days
AUDIT_LOG_ENABLED=true          # Enable audit logging

#====================================================================================================
# BACKUP & DISASTER RECOVERY
#====================================================================================================

# Backup Configuration
BACKUP_ENABLED=true
BACKUP_FREQUENCY=daily
BACKUP_RETENTION_DAYS=30
BACKUP_STORAGE_TYPE=s3          # s3, gcs, azure
BACKUP_STORAGE_URL=s3://your-backup-bucket

# Disaster Recovery
DR_ENABLED=false
DR_FALLBACK_ENDPOINT=https://your-dr-instance.com
DR_NOTIFICATION_EMAIL=admin@yourcompany.com

#====================================================================================================
# DEVELOPMENT & TESTING ENVIRONMENTS
#====================================================================================================

# Environment Configuration
ENVIRONMENT=production          # development, staging, production
NODE_ENV=production

# Development Overrides (uncomment for dev/staging)
# DEBUG_MODE=true
# TEST_MODE=true  
# BATCH_SIZE=3
# MAX_LEADS_PER_RUN=10
# USE_MOCK_DATA=true

#====================================================================================================
# CUSTOM INTEGRATIONS & EXTENSIONS
#====================================================================================================

# Custom Webhook Endpoints
CUSTOM_LEAD_WEBHOOK=https://your-crm.com/api/leads/webhook
CUSTOM_ANALYTICS_WEBHOOK=https://your-analytics.com/api/events

# Third-party Service Integrations
CALENDLY_API_KEY=your_calendly_api_key
INTERCOM_ACCESS_TOKEN=your_intercom_token
TYPEFORM_API_KEY=your_typeform_api_key

# Custom AI Model Endpoints
CUSTOM_AI_ENDPOINT=https://your-custom-ai-service.com/api/score
CUSTOM_AI_API_KEY=your_custom_ai_api_key

#====================================================================================================
# MONITORING & ALERTING
#====================================================================================================

# Health Monitoring
HEALTH_CHECK_ENABLED=true
HEALTH_CHECK_INTERVAL=300       # 5 minutes
UPTIME_MONITOR_URL=https://your-uptime-monitor.com/webhook

# Error Alerting
ERROR_ALERT_ENABLED=true
ERROR_THRESHOLD=5               # Alert after 5 consecutive errors
ALERT_EMAIL=alerts@yourcompany.com
PAGERDUTY_SERVICE_KEY=your_pagerduty_service_key

# Performance Monitoring
PERFORMANCE_ALERT_ENABLED=true
MAX_EXECUTION_TIME=1800         # 30 minutes
MEMORY_LIMIT_MB=2048

#====================================================================================================
# MONETIZATION & BILLING (For Service Providers)
#====================================================================================================

# Usage Tracking
USAGE_TRACKING_ENABLED=false    # Enable for SaaS offerings
BILLING_API_ENDPOINT=https://your-billing-service.com/api/usage
BILLING_API_KEY=your_billing_api_key

# License Management
LICENSE_TIER=ultimate           # starter, professional, enterprise, ultimate
MONTHLY_LEAD_LIMIT=5000         # Leads per month limit
FEATURE_RESTRICTIONS=none       # Comma-separated restricted features

